{
    "name": "kgulpinar/laravel-admin-subscriptions",
    "description": "Subscriptions is a flexible plans and subscription management system ( for SAAS ) for Laravel Admin",
    "type": "library",
    "keywords": [
        "plan",
        "value",
        "laravel",
        "feature",
        "database",
        "recurring",
        "subscription",
        "laravel-admin"
    ],
    "license": "MIT",
    "homepage": "https://kgulpinar.org",
    "support": {
        "email": "koraygulpinar@gmail.com",
        "issues": "https://github.com/rinvex/laravel-subscriptions/issues",
        "source": "https://github.com/rinvex/laravel-subscriptions",
        "docs": "https://github.com/kgulpinar/laravel-subscriptions/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Koray Gulpinar",
            "homepage": "https://koray.org",
            "email": "kgulpinar@gmail.com"
        },
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/laravel-subscriptions/contributors"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "illuminate/console": "^7.0.0",
        "illuminate/database": "^7.0.0",
        "illuminate/support": "^7.0.0",
        "rinvex/laravel-cacheable": "^4.0.0",
        "rinvex/laravel-support": "^4.0.0",
        "spatie/eloquent-sortable": "^3.7.0",
        "spatie/laravel-sluggable": "^2.2.0",
        "spatie/laravel-translatable": "^4.2.0",
        "encore/laravel-admin": ">=1.6"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.27.0",
        "illuminate/container": "^7.0.0",
        "phpunit/phpunit": "^9.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Rinvex\\Subscriptions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Subscriptions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Subscriptions\\Providers\\SubscriptionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
